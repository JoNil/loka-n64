{
    "rust-analyzer.checkOnSave.overrideCommand": [
        "cargo",
        "+nightly",
        "clippy",
        "-Z=build-std=core,alloc",
        "--message-format=json",
        "--target=mips-nintendo64-none.json",
        "-p",
        "game",
        "--target-dir",
        "target/analyzer",
        "--",
        "--allow",
        "clippy::unusual_byte_groupings"
    ],
}
